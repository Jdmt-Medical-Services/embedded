{
  "name": "jdmt-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aws-sdk": "^2.358.0",
    "babel-polyfill": "^6.26.0",
    "chart.js": "^2.7.2",
    "dotenv": "^6.1.0",
    "concurrently": "^4.1.0",
    "history": "^4.7.2",
    "react": "^16.4.1",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.4.1",
    "react-feather": "^1.1.4",
    "react-google-maps": "^9.4.5",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "s3": "^4.4.0",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "deploy": "npm run deploy:frontend && npm run build:lambda && npm run deploy:lambda",
    "deploy:frontend": "npm run clean && parcel build ./src/index.html --out-dir build && npx ./lambda/deploy.js && npm run clean",
    "build:lambda": "sam package --template-file lambda/template.yml  --output-template-file lambda/packaged.yaml  --s3-bucket jdmt-deployment",
    "deploy:lambda": "sam deploy --template-file lambda/packaged.yaml  --stack-name jdmt  --capabilities CAPABILITY_IAM ",
    "start:frontend": "NODE_ENV=DEBUG parcel ./src/index.html --open",
    "start:lambda": "sam local start-api -t ./lambda/template.yml",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:lambda\"",
    "clean": "rm -rfd ./build"
  },
  "devDependencies": {
    "@types/googlemaps": "^3.30.16",
    "@types/markerclustererplus": "^2.1.33",
    "@types/react": "^16.7.6",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "parcel": "^1.10.3"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/prop-types": false
    },
    "globals": {
      "document": false,
      "fetch": true
    }
  }
}
